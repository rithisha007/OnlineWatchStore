{
  "extends": [
    "development"
  ],
  "hints": {
    "axe/name-role-value": [
      "default",
      {
        "link-name": "off",
        "button-name": "off"
      }
    ],
    "compat-api/html": [
      "default",
      {
        "ignore": [
          "iframe[loading]"
        ]
      }
    ],
    "axe/text-alternatives": "off",
    "no-inline-styles": "off",
    "axe/forms": [
      "default",
      {
        "select-name": "off",
        "label": "off"
      }
    ],
    "detect-css-reflows/layout": "off",
    "axe/parsing": [
      "default",
      {
        "duplicate-id-aria": "off"
      }
    ],
    "meta-viewport": "off",
    "compat-api/css": [
      "default",
      {
        "ignore": [
          "-ms-filter",
          "-ms-text-size-adjust",
          "width: fit-content"
        ]
      }
    ],
    "disown-opener": "off",
    "axe/aria": [
      "default",
      {
        "aria-required-children": "off",
        "aria-required-parent": "off"
      }
    ]
  }
}